{
    "name": "joonas1234/nova-combined-fields",
    "description": "Flexible Content & Repeater Fields for Laravel Nova.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "flexible",
        "repeat",
        "group",
        "layout"
    ],
    "authors": [
        {
            "name": "Joonas Kämäräinen",
            "email": "joonas@ninjami.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Joonas1234\\NovaCombinedFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joonas1234\\NovaCombinedFields\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
